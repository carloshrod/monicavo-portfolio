---
const { path } = Astro.props;
---

<a {...Astro.props} href={path} aria-label='Go back'>
	<svg
		class='w-6 h-6 text-stone-950'
		aria-hidden='true'
		xmlns='http://www.w3.org/2000/svg'
		width='24'
		height='24'
		fill='none'
		viewBox='0 0 24 24'
	>
		<path
			stroke='currentColor'
			stroke-linecap='round'
			stroke-linejoin='round'
			stroke-width='2'
			d='m14 8-4 4 4 4'></path>
	</svg>
</a>
